{
  "name": "@sentinel/react",
  "version": "0.1.0",
  "description": "",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.mjs",
  "types": "build/types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.mjs"
      },
      "require": {
        "types": "./build/types/index.d.ts",
        "default": "./build/cjs/index.js"
      }
    }
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "scripts": {
    "build:transpile": "tsup",
    "build:types": "tsc -p tsconfig.types.json",
    "build:watch": "tsup --watch",
    "build:watch:types": "tsc -p tsconfig.types.json --watch",
    "watch": "pnpm build:watch & pnpm build:watch:types",
    "build:lib": "pnpm build:transpile & pnpm build:types"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@sentinel/browser-utils": "workspace:*",
    "@sentinel/core": "workspace:*",
    "@sentinel/shared": "workspace:*",
    "@types/react": "^19.1.9"
  },
  "dependencies": {
    "react": "^19.1.1"
  }
}
