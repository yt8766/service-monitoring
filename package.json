{
  "name": "service-monitoring",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:watch": "turbo build:watch",
    "build:lib": "node ./scripts/build.js && turbo build:lib",
    "build:server": "turbo build:server",
    "start:dev": "turbo start:dev",
    "dev": "turbo dev",
    "build": "pnpm build:server && pnpm build:lib",
    "build:shared": "node ./scripts/build.js -f shared && pnpm  -C ./packages/shared build:lib",
    "build:core": "node ./scripts/build.js -f core && pnpm -C ./packages/core build:lib",
    "build:browser-utils": "node ./scripts/build.js -f browser-utils && pnpm -C ./packages/browser-utils build:lib",
    "build:browser": "node ./scripts/build.js -f browser && pnpm -C ./packages/browser build:lib",
    "lint:prettier": "prettier --write \"**/*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:eslint": "eslint --fix --ext .js,.ts,.mjs,.cjs,.tsx,.vue .",
    "spellcheck": "cspell lint  --no-progress --show-suggestions --no-summary --no-color \"(packages|apps)/**/*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}\"",
    "prepare": "husky",
    "docker:start": "docker compose -p sentinel-monitor -f .dccontainer/docker-compose.yaml up -d",
    "test": "pnpm lint:prettier && pnpm lint:eslint && pnpm spellcheck"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cspell/dict-lorem-ipsum": "^4.0.5",
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.2.0",
    "cspell": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tsup": "^8.5.0",
    "turbo": "^2.5.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vanilla": "^0.0.5"
  }
}
